{
  "pipeline": {
    "description": "Test grok pattern",
    "processors": [
      {
        "grok": {
          "field": "message",
          "patterns": [
            "\\{\"timestamp\": \"%{TIMESTAMP_ISO8601:timestamp}\", \"id\": %{NUMBER:id}, \"class\": \"%{WORD:class}\", \"event\": \"%{WORD:event}\", \"connection_id\": %{NUMBER:connection_id}, \"account\": \\{ \"user\": \"%{DATA:account_user}\", \"host\": \"%{DATA:account_host}\"\\}, \"connection_data\": \\{ \"login\": \\{ \"user\": \"%{DATA:login_user}\", \"os\": \"%{DATA:login_os}\", \"ip\": \"%{IP:login_ip}\", \"proxy\": \"%{DATA:login_proxy}\"\\}, \"connection_type\": \"%{WORD:connection_type}\", \"status\": %{NUMBER:status}, \"db\": \"%{WORD:db}\", \"connection_attributes\": \\{ \"_runtime_version\": \"%{DATA:runtime_version}\", \"_client_version\": \"%{DATA:client_version}\", \"client_license\": \"%{WORD:client_license}\", \"_runtime_vendor\": \"%{WORD:runtime_vendor}\", \"_client_name\": \"%{DATA:client_name}\"\\}\\}\\}"
          ]
        }
      }
    ]
  },
