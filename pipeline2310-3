    {
      "_source": {
        "message": "{\"timestamp\": \"2024-10-16 12:45:17\", \"id\": 5, \"class\": \"connection\", \"event\": \"connect\", \"connection_id\": 68368, \"account\": { \"user\": \"jira\", \"host\": \"test-1.dev.com\"}, \"connection_data\": { \"login\": { \"user\": \"jira\", \"os\": \"\", \"ip\": \"10.0.0.0\", \"proxy\": \"\"}, \"connection_type\": \"ssl\", \"status\": 0, \"db\": \"jiradb\", \"connection_attributes\": { \"_runtime_version\": \"11.0.10\", \"_client_version\": \"8.2.0\", \"client_license\": \"GPL\", \"_runtime_vendor\": \"AdoptopenJDK\", \"_client_name\": \"MySQL Connector\"}}}"
